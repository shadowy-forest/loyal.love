{
  "htmlWhitespaceSensitivity": "ignore",
  "htmlSelfClosingTags": true,
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "htmlParser": "html",
  "htmlVoidTags": ["area", "base", "br", "col", "embed", "hr", "img", "input", "link", "meta", "param", "source", "track", "wbr"],
  "preserveDoctypeCase": true
} 